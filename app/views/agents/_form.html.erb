<%= form_for(@agent) do |f| %>
	<%= error_messages_for(@agent) %>

  <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name %>
  </div>
  <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name %>
  </div>
  <div class="field">
    <%= f.label :email %><br />
    <%= f.text_field :email %>
  </div>
  <div class="field">
    <%= f.label :phone %><br />
    <%= f.number_field :phone %>
  </div>
  <div class="field">
    <%= f.label :website_url %><br />
    <%= f.text_area :website_url %>
  </div>
  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
